{{extend './layout.art'}}
{{block 'title'}} node+express项目 {{/block}}
{{block 'content'}}
{{total}}
<ul class="list-group">
  {{each data}}
  <li class="list-group-item">
    <p>
      {{$value.post_title}}
    </p>
    <p>
      {{$value.post_date | date 'yyyy-MM-dd hh-mm:ss'}}
    </p>
  </li>
  {{/each}}
</ul>
{{/block}}